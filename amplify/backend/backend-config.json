{
    "function": {
        "amplifyreactotpCreateAuthChallenge": {
            "build": true,
            "providerPlugin": "awscloudformation",
            "service": "Lambda"
        },
        "amplifyreactotpDefineAuthChallenge": {
            "build": true,
            "providerPlugin": "awscloudformation",
            "service": "Lambda"
        },
        "amplifyreactotpVerifyAuthChallengeResponse": {
            "build": true,
            "providerPlugin": "awscloudformation",
            "service": "Lambda"
        },
        "amplifyreactotpPreSignup": {
            "build": true,
            "providerPlugin": "awscloudformation",
            "service": "Lambda"
        }
    },
    "auth": {
        "amplifyreactotp": {
            "service": "Cognito",
            "providerPlugin": "awscloudformation",
            "dependsOn": [
                {
                    "category": "function",
                    "resourceName": "amplifyreactotpCreateAuthChallenge",
                    "triggerProvider": "Cognito",
                    "attributes": [
                        "Arn",
                        "Name"
                    ]
                },
                {
                    "category": "function",
                    "resourceName": "amplifyreactotpDefineAuthChallenge",
                    "triggerProvider": "Cognito",
                    "attributes": [
                        "Arn",
                        "Name"
                    ]
                },
                {
                    "category": "function",
                    "resourceName": "amplifyreactotpVerifyAuthChallengeResponse",
                    "triggerProvider": "Cognito",
                    "attributes": [
                        "Arn",
                        "Name"
                    ]
                },
                {
                    "category": "function",
                    "resourceName": "amplifyreactotpPreSignup",
                    "triggerProvider": "Cognito",
                    "attributes": [
                        "Arn",
                        "Name"
                    ]
                }
            ],
            "customAuth": true
        }
    },
    "hosting": {
        "amplifyhosting": {
            "service": "amplifyhosting",
            "type": "cicd"
        }
    }
}